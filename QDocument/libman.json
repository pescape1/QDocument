{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "select2@4.0.5",
      "destination": "wwwroot/lib/select2/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
